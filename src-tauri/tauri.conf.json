{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Aisle3",
  "version": "0.1.0",
  "identifier": "com.aisle3.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../build"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "Aisle 3",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600
      }
    ],
    "security": {
      "csp": null,
      "capabilities": ["default"]
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "updater": {
      "active": true,
      "endpoints": [
          "https://github.com/bryansh/Aisle3/releases/latest/download"
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDhEQUFFQUQ5NkE2QzFFODcKUldUK0FnVmhFUmdrRlFTc0JMWmZOTUJoVkVSQ2VkZWZVK3lsZmwvUmRKYUhQemJUdGtrVXBZVmcK"
    }
  }
}