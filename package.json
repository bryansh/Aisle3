  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "tauri": "tauri",
    "release": "node scripts/release.cjs",
    "release:patch": "node scripts/release.cjs patch",
    "release:minor": "node scripts/release.cjs minor",
    "release:major": "node scripts/release.cjs major",
    "release:check": "node scripts/release-check.cjs",
    "test": "vitest --config vitest.config.js",
    "test:run": "vitest run --config vitest.config.js",
    "test:ui": "vitest --ui --config vitest.config.js",
    "test:coverage": "vitest run --coverage --config vitest.config.js",
    "test:watch": "vitest --watch --config vitest.config.js",
    "test:rust": "cd src-tauri && cargo test",
    "test:rust:watch": "cd src-tauri && cargo watch -x test",
    "test:all": "npm run test:run && npm run test:rust",
    "test:components": "vitest run src/tests/components --config vitest.config.js",
    "test:stores": "vitest run src/tests/stores --config vitest.config.js",
    "test:services": "vitest run src/tests/services --config vitest.config.js",
    "test:integration": "vitest run src/tests/integration --config vitest.config.js",
    "test:e2e": "playwright test --config=playwright.config.js",
    "test:e2e:ui": "playwright test --ui --config=playwright.config.js",
    "test:e2e:headed": "playwright test --config=playwright.config.js --headed",
    "test:e2e:debug": "playwright test --config=playwright.config.js --debug",
    "test:playwright:install": "playwright install",
    "count-lines": "node src/utils/count-lines.js",
    "count-lines:detailed": "node src/utils/count-lines.js --detailed"
  },
